{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "default": {},
    "title": "Root Schema",
    "required": [
        "title",
        "salary",
        "equity",
        "compHandle"
    ],
    "properties": {
        "title": {
            "type": "string",
            "default": "",
            "title": "The title Schema",
            "examples": [
                "Example"
            ],
            "maxLength": "30",
            "minLength": "1"
        },
        "salary": {
            "type": "integer",
            "default": 0,
            "title": "The salary Schema",
            "examples": [
                100000
            ],
            "minimum": "0"
        },
        "equity": {
            "type": "number",
            "default": 0.0,
            "title": "The equity Schema",
            "examples": [
                0.3
            ],
            "minimum": "0",
            "maximum": "1"
        },
        "compHandle": {
            "type": "string",
            "default": "",
            "title": "The compHandle Schema",
            "examples": [
                "example"
            ]
        }
    },
    "examples": [{
        "title": "Example",
        "salary": 100000,
        "equity": 0.3,
        "compHandle": "example"
    }]
}